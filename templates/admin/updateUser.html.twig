{% extends 'base.html.twig' %}

{% block title %}Dashboard{% endblock %}    

{% block body %}

<h1>Edit User</h1>
    {{ form_start(form) }}
    {{ form_row(form.first_name) }}
    {{ form_row(form.last_name) }}
    {{ form_row(form.email) }}
    {{ form_row(form.password) }}
    {{ form_row(form.roles) }}
    <button type="submit" class="btn btn-primary">Save</button>
    {{ form_end(form) }}

{% endblock %}
